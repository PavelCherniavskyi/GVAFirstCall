<Window x:Class="FirstEvent.View.MainWindows.MainTestWindow"
        x:Name="This"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:i="http://schemas.microsoft.com/expression/2010/interactivity"
        xmlns:view="clr-namespace:FirstEvent.View"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        mc:Ignorable="d"
        Title="Testing mode" Height="950" MinWidth="820" Width="820" ResizeMode="CanResizeWithGrip" WindowStartupLocation="CenterScreen" Icon="/FirstEvent;component/View/Images/dolphin.ico">
    <i:Interaction.Triggers>
        <i:EventTrigger EventName="Closing">
            <i:InvokeCommandAction Command="{Binding CancelButtonCommand}" />
        </i:EventTrigger>
    </i:Interaction.Triggers>
    <ScrollViewer>
        <StackPanel Orientation="Vertical">
            <Expander Margin="5" BorderThickness="2" BorderBrush="#FFACCEE2" IsExpanded="False" Padding="0,0,0,5"> 
                <Expander.Header>
                    <TextBlock Text="General info" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
                </Expander.Header>
                <StackPanel Orientation="Horizontal" Margin="-24,-17,0,0" HorizontalAlignment="Center">
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="4" Margin="31,10,0,0" Width="230">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Number:" DockPanel.Dock="Left" Margin="0,0,0,0"/>
                            <TextBox  DockPanel.Dock="Right" Width="150" Text="400245" Height="20" IsEnabled="False" ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Inner case number</TextBlock>
                                        <TextBlock Text="This is inner case number in database.&#x0a;Do not pay attention on it. For most of the cases you will need case reference number below."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Case:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                            <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="O" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Actual case number</TextBlock>
                                        <TextBlock Text="When first event will be created this field gets unique number in database.&#x0a;Format: M-123456&#x0a;For now it is empty."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="17,10,0,0" Width="232">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Doc Date:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                            <xctk:DateTimePicker DockPanel.Dock="Right" IsEnabled="False" Width="150" Format="Custom" FormatString="yyyy.MM.dd HH:mm:ss" Background="#FFF0F0F0" 
                                                Value="{Binding GeneralInfo.DocDateTime, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <xctk:DateTimePicker.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Document date</TextBlock>
                                        <TextBlock Text="Time when first event was created.&#x0a;You can't change it."/>
                                    </StackPanel>
                                </xctk:DateTimePicker.ToolTip>
                            </xctk:DateTimePicker>
                        </DockPanel>
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Duty OPS:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                            <TextBox Text="{Binding GeneralInfo.DutyOps, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" Height="20" DockPanel.Dock="Right" Width="150" IsEnabled="False"
                                     ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Duty operator field</TextBlock>
                                        <TextBlock Text="You will have your peronal account in dolphin.&#x0a;Here your name will be indicated so that everyone would know who created this first event.&#x0a;No one can't create first event under another account so idea to change it doesn't make sence."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="21,10,0,0" Width="230">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Event Date:" DockPanel.Dock="Left" Margin="-3,0,0,0" />
                            <xctk:DateTimePicker DockPanel.Dock="Right" Width="150" Format="Custom" FormatString="yyyy.MM.dd HH:mm:ss" ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000"
                                                Value="{Binding GeneralInfo.EventDateTime, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                <xctk:DateTimePicker.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Event date</TextBlock>
                                        <TextBlock Text="Time when a case occurred.&#x0a;You must correct this field and indicate the exact date if the event did not occure today."/>
                                    </StackPanel>
                                </xctk:DateTimePicker.ToolTip>
                            </xctk:DateTimePicker>
                        </DockPanel>
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Duty doctor:" DockPanel.Dock="Left" Margin="-3,0,0,0" />
                            <Button DockPanel.Dock="Right" Width="12" Height="20" Margin="-13,0,0,0" Panel.ZIndex="1" Content="X" Command="{Binding GeneralInfo.CancelDocTextField}" />
                            <Button DockPanel.Dock="Right" Width="12" Height="20" Margin="-25,0,9,0" Panel.ZIndex="1" Content="..." Command="{Binding GeneralInfo.ShowDocListWindow}"/>
                            <TextBox DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,0,0" ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000"
                                 IsEnabled="{Binding GeneralInfo.IsDutyDocEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding GeneralInfo.DutyDoctor.FullName, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding GeneralInfo.EnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Doctor on duty</TextBlock>
                                        <TextBlock Text="You must choose a doctor who is on duty today from database"/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/info-2-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="To do it faster you can enter a contact code from database if you know it or try to type first leters of last name&#x0a;and it will automaticaly find it." FontStyle="Italic"/>
                                        </StackPanel>
                                        
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                </StackPanel>
            </Expander>
            <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="#FFACCEE2" IsExpanded="False" Padding="0,0,0,5" >
                <Expander.Header>
                    <TextBlock Text="Caller" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
                </Expander.Header>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Margin="10,-9,1,-5" VerticalAlignment="Bottom" >
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Last Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.LastName, UpdateSourceTrigger=LostFocus}"
                                ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Caller last name</TextBlock>
                                        <TextBlock Text="Last name of who is calling."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="First Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.FirstName, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Caller first name</TextBlock>
                                        <TextBlock Text="First name of who is calling."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Middle Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.MiddleName, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Caller middle name</TextBlock>
                                        <TextBlock Text="Middle name of who is calling."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Relation to ..." DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" Command="{Binding Caller.CancelRelationToSubscrListWindow}" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." Command="{Binding Caller.ShowRelationToSubscrListWindow}" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  
                                 IsEnabled="{Binding Caller.IsRelToSubscrEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Caller.RelToSub.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding Caller.RelEnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Relation to caller</TextBlock>
                                        <TextBlock Text="This field tells us who we are dealing with in relation to the subscriber."/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/info-2-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="To do it faster you can enter a relation code from database if you know it &#x0a;or try to type first leters and it will try to found relation automaticaly." FontStyle="Italic"/>
                                        </StackPanel>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Country:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" Command="{Binding Caller.CancelCountryListWindow}" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." Command="{Binding Caller.ShowCountryListWindow}" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"
                                  IsEnabled="{Binding Caller.IsCountryEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Caller.Country.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding Caller.CountryEnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Country</TextBlock>
                                        <TextBlock Text="Indicates where the caller is."/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/info-2-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="To do it faster you can enter a country code from database if you know it &#x0a;or try to type first leters and it will try to find country automaticaly." FontStyle="Italic"/>
                                        </StackPanel>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Region:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" Command="{Binding Caller.CancelRegionListWindow}" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." Command="{Binding Caller.ShowRegionListWindow}" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"
                                  IsEnabled="{Binding Caller.IsRegionEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Caller.Region.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding Caller.RegionEnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Region</TextBlock>
                                        <TextBlock Text="Indicates where the caller is."/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/info-2-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="To do it faster you can enter a region code from database if you know it &#x0a;or try to type first leters and it will try to find region automaticaly." FontStyle="Italic"/>
                                        </StackPanel>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="City:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" Command="{Binding Caller.CancelCityListWindow}" />
                            <Button DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." Command="{Binding Caller.ShowCityListWindow}" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"
                                  IsEnabled="{Binding Caller.IsCityEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Caller.City.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding Caller.CityEnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">City</TextBlock>
                                        <TextBlock Text="Indicates where the caller is."/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/info-2-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="To do it faster you can enter a city code from database if you know it &#x0a;or try to type first leters and it will try to find it.&#x0a;Note that if you select a city, region and country fields will be settled automaticaly (you can save your time)." FontStyle="Italic"/>
                                        </StackPanel>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Location info:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.LocationInfo, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Location information</TextBlock>
                                        <TextBlock Text="You can clarify exactly where the caller is, if he is located not is the city. &#x0a;Any orientation on terrain, village name, name of this area?"/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Place of stay:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.PlaceOfStay, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Place of stay information</TextBlock>
                                        <TextBlock Text="Exact place where the caller is. &#x0a;Is it hotel, hostel, private property, appartments?"/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Room:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.Room, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Room number</TextBlock>
                                        <TextBlock Text="Number of appartments or room if any."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="True" VerticalAlignment="Center" Height="20" ></DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Caler ID:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Caller.CallerId, UpdateSourceTrigger=LostFocus}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Identity number</TextBlock>
                                        <TextBlock Text="If necessary."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                </StackPanel>
            </Expander>
            <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="#FFACCEE2" IsExpanded="False" Padding="0,0,0,5">
                <Expander.Header>
                    <TextBlock Text="Membership and Subscriber" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
                </Expander.Header>
                <StackPanel Orientation="Vertical">
                    <StackPanel Orientation="Horizontal" Margin="-16,6,0,8" HorizontalAlignment="Center">
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                            <TextBlock Text="Membership:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" Command="{Binding Membership.CancelInsuranceListWindow}" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-32,0,22,0" Panel.ZIndex="1" Content="..." Command="{Binding Membership.ShowHotelListWindow}" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-9,0,-4,0" Panel.ZIndex="1" Content="O" />
                            <TextBox  DockPanel.Dock="Right" Width="150" Height="20" Margin="0,0,3,0" 
                                  IsEnabled="{Binding Membership.IsInsuranceEnabled, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Membership.Insurance.Name, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.InputBindings>
                                    <KeyBinding Key="Enter" Command="{Binding Membership.InsuranceEnterKeyCommand}"/>
                                </TextBox.InputBindings>
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Membership</TextBlock>
                                        <TextBlock Text="You should select insurance company of subscriber."/>
                                        <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                        <StackPanel Orientation="Horizontal">
                                            <Image Margin="5" Source="../Images/warning-4-32.ico" Width="25" Height="25"/>
                                            <TextBlock Text="This field is required!" FontStyle="Italic"/>
                                        </StackPanel>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                            <TextBlock Text="M.O. Case№:" DockPanel.Dock="Left" Margin="14,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,-4,0"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Outer reference number</TextBlock>
                                        <TextBlock Text="In this is a request from another company you should indicate here their reference number on that case"/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                            <TextBlock Text="Valid Subscr." DockPanel.Dock="Left" Margin="25,0,0,0" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-5,0,-8,0" Panel.ZIndex="1" Content="X" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-14,0,4,0" Panel.ZIndex="1" Content="..." />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="9,0,-22,0" Panel.ZIndex="1" Content="O" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="11,0,9,0"
                                      ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                <TextBox.ToolTip>
                                    <StackPanel>
                                        <TextBlock FontWeight="Bold">Subscriber validation</TextBlock>
                                        <TextBlock Text="Some of insurance company sends us lists of insured people.&#x0a;In some cases you can find subscriber here and save time for policy validation.&#x0a;This field doesn't work in testing mode."/>
                                    </StackPanel>
                                </TextBox.ToolTip>
                            </TextBox>
                        </DockPanel>
                    </StackPanel>
                    <Separator HorizontalAlignment="Stretch" Height="1"/>
                    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Margin="10,4,1,-6" >
                        <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                            <DockPanel VerticalAlignment="Center" LastChildFill="False">
                                <TextBlock Text="Last Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                                <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" Text="{Binding Subscriber.LastName}"
                                          ToolTipService.ShowOnDisabled="True" ToolTipService.ShowDuration="100000">
                                    <TextBox.ToolTip>
                                        <StackPanel>
                                            <TextBlock FontWeight="Bold">Last name</TextBlock>
                                            <TextBlock Text="Last name of subscriber"/>
                                            <Border BorderBrush="Silver" BorderThickness="0,1,0,0" Margin="0,8" />
                                            <StackPanel Orientation="Horizontal">
