                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-27,0,14,0" Panel.ZIndex="1" Content="X" />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-36,0,26,0" Panel.ZIndex="1" Content="..." />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="O" />
                        <TextBox  DockPanel.Dock="Right" Height="20" Width="294" Margin="0,0,4,0"  />
                    </DockPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="17,10,0,0" Width="191">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Country:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="X" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-25,0,12,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" Width="130" Height="20" Margin="0,0,0,0" />
                    </DockPanel>
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Region:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="X" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-25,0,12,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" Width="130" Height="20" Margin="0,0,0,0" />
                    </DockPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="21,10,0,0" Width="209">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="City:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="X" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-25,0,12,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" Width="130" Height="20"  Margin="0,0,0,0" />
                    </DockPanel>
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Location info:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" Width="130" Height="20"  Margin="0,0,0,0" />
                    </DockPanel>
                </StackPanel>
            </StackPanel>
        </Expander>
        <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="Coral" IsExpanded="False" Padding="0,0,0,5">
            <Expander.Header>
                <TextBlock Text="Contacts" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0" />
            </Expander.Header>
            <StackPanel>
                <StackPanel Orientation="Horizontal" view:MarginSetter.Margin="3" Margin="10,0">
                    <Button Name="ContactsAddBtn" Height="25" Width="25" BorderBrush="#00707070" Background="#00DDDDDD" >
                        <Image Source="add1.ico"/>
                    </Button>
                    <Button Name="ContactsCopyBtn" Height="25" Width="25" BorderBrush="#00707070" Background="#00DDDDDD" >
                        <Image Source="copy.ico"/>
                    </Button>
                    <Button Name="ContactsDeleteBtn" Height="25" Width="25" BorderBrush="#00707070" Background="#00DDDDDD" >
                        <Image Source="delete.ico"/>
                    </Button>
                </StackPanel>
                <DataGrid Name="DataGridContacts" Margin="10,0" CanUserResizeRows="False" 
                          ItemsSource="{Binding}" AutoGenerateColumns="False" HorizontalGridLinesBrush="#FFF0F0F0" VerticalGridLinesBrush="#FFF0F0F0" 
                          CanUserReorderColumns="False" CanUserResizeColumns="False" CanUserSortColumns="False" RowHeaderWidth="0" CanUserAddRows="False">
                    <DataGrid.Resources>
                        <Style TargetType="{x:Type DataGridCell}">
                            <Style.Triggers>
                                <Trigger Property="DataGridCell.IsSelected" Value="True">
                                    <Setter Property="Background" Value="#FFA0A0A0" />
                                </Trigger>
                            </Style.Triggers>
                        </Style>
                    </DataGrid.Resources>
                    <!--<DataGrid.DataContext>
                        mai
                    </DataGrid.DataContext>-->
                    <DataGrid.Columns>
                        <DataGridTextColumn Binding="{Binding Path=Id }" Header="№" Width="23"/>
                        <DataGridTextColumn Binding="{Binding Path=RelationToSubscr}" Header="Relation to Subscriber" Width="130"/>
                        <DataGridTextColumn Binding="{Binding Path=Name}" Header="Name" Width="150"/>
                        <DataGridTemplateColumn  x:Name="ContactsTypeOfContact" Header="Type of contact" Width="120">
                            <DataGridTemplateColumn.CellTemplate>
                                <DataTemplate>
                                    <ComboBox ItemsSource="{Binding TypeOfContact}" />
                                </DataTemplate>
                            </DataGridTemplateColumn.CellTemplate>
                        </DataGridTemplateColumn>
                        <DataGridTextColumn Binding="{Binding Path=ContactNumber}" Header="Contact" Width="100"/>
                        <DataGridTextColumn Binding="{Binding Path=Info}" Header="Info" Width="225"/>
                    </DataGrid.Columns>
                    

                </DataGrid>
            </StackPanel>
        </Expander>
        
    </StackPanel>
        
        

</Window>



