<Window x:Class="FirstEvent.View.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:wpfTool="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        xmlns:view="clr-namespace:FirstEvent.View"
        xmlns:local="clr-namespace:FirstEvent.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="950" Width="800" ResizeMode="CanResize" WindowStartupLocation="CenterScreen" Topmost="True">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    
    <StackPanel Orientation="Vertical">

        <Expander Margin="5" BorderThickness="2" BorderBrush="Coral" IsExpanded="False" Padding="0,0,0,5">
            <Expander.Header>
                <TextBlock Text="General info" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
            </Expander.Header>
            <StackPanel Orientation="Horizontal" Margin="-7,-17,0,0" >
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="4" Margin="31,10,0,0" Width="230">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Number:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" Width="150" Height="20" Text="M-369524" IsReadOnly="True" Background="#FFF0F0F0" />
                    </DockPanel>
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Case:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="O" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0" />
                    </DockPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="17,10,0,0" Width="232">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Doc Date:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <wpfTool:DateTimePicker Name="GeneralInfoDocDate" DockPanel.Dock="Right" IsReadOnly="True" Width="150" Format="Custom" FormatString="yyyy.MM.dd HH:mm:ss" Background="#FFF0F0F0" />
                    </DockPanel>
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Duty OPS:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <TextBox Text="Pavel Cherniavskyi" Height="20" DockPanel.Dock="Right" Width="150" IsReadOnly="True" Background="#FFF0F0F0" />
                    </DockPanel>
                </StackPanel>
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="21,10,0,0" Width="230">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Event Date:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <wpfTool:DateTimePicker DockPanel.Dock="Right" Width="150" Format="Custom" FormatString="yyyy.MM.dd HH:mm:ss"/>
                    </DockPanel>
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Duty doctor:" DockPanel.Dock="Left" Margin="0,0,0,0" />
                        <Button DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="X" Command="{Binding CancelDocTextField}" />
                        <Button DockPanel.Dock="Right" MinWidth="10" Margin="-25,0,12,0" Panel.ZIndex="1" Content="..." Command="{Binding ShowDocListWindow}"/>
                        <TextBox Name="DutyDoctor" DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,0,0" 
                                 IsReadOnly="{Binding IsDutyDocReadOnly, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" 
                                 Text="{Binding Path=DutyDoctor, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" />
                    </DockPanel>
                </StackPanel>
            </StackPanel>
        </Expander>
        <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="Coral" IsExpanded="False" Padding="0,0,0,5" >
            <Expander.Header>
                <TextBlock Text="Caller" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
            </Expander.Header>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Margin="10,-9,1,-5" VerticalAlignment="Bottom" >
                <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Last Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="First Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Middle Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Relation to ..." DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <Button x:Name="RelationToCallerClear" DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                        <Button x:Name="RelationToClr" DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                </StackPanel>
                <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Country:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <Button x:Name="CountryOfCallerClr"  DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                        <Button x:Name="CountryOfCaller"  DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Region:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <Button x:Name="RegionOfCallerClr"  DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                        <Button x:Name="RegionOfCaller"  DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="City:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <Button x:Name="CityOfCallerClr"  DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                        <Button x:Name="CityOfCaller"  DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Location info:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                </StackPanel>
                <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                    <DockPanel VerticalAlignment="Center" LastChildFill="False">
                        <TextBlock Text="Place of stay:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Room:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="True" VerticalAlignment="Center" Height="20" ></DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                        <TextBlock Text="Caler ID:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                    </DockPanel>
                </StackPanel>
            </StackPanel>
        </Expander>
        <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="Coral" IsExpanded="False" Padding="0,0,0,5">
            <Expander.Header>
                <TextBlock Text="Membership and Subscriber" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
            </Expander.Header>
            <StackPanel Orientation="Vertical">
                <StackPanel Orientation="Horizontal" view:MarginSetter.Margin="6">
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                        <TextBlock Text="Membership:" DockPanel.Dock="Left" Margin="11,0,0,0" />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-27,0,14,0" Panel.ZIndex="1" Content="X" />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-36,0,26,0" Panel.ZIndex="1" Content="..." />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-13,0,0,0" Panel.ZIndex="1" Content="O" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,4,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                        <TextBlock Text="M.O. Case№:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,6,0"  />
                    </DockPanel>
                    <DockPanel LastChildFill="False" VerticalAlignment="Center" Width="241">
                        <TextBlock Text="Valid Subscr." DockPanel.Dock="Left" Margin="3,0,0,0" />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-30,0,17,0" Panel.ZIndex="1" Content="X" />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-39,0,29,0" Panel.ZIndex="1" Content="..." />
                        <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-16,0,3,0" Panel.ZIndex="1" Content="O" />
                        <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,9,0"  />
                    </DockPanel>
                </StackPanel>
                <Separator HorizontalAlignment="Stretch" Height="1"/>
                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center"  Margin="10,4,1,-6" >
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Last Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="First Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Middle Name:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                        </DockPanel>
                        <DockPanel >
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Left" Margin="0">
                                <TextBlock Text="Date of Birth:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                                <DatePicker DockPanel.Dock="Right" Margin="8,0,0,0" Height="22" Width="90" FontSize="10" VerticalContentAlignment="Center" />
                            </DockPanel>
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Right" >
                                <TextBox DockPanel.Dock="Right" Width="20" Margin="0,0,10,0" Height="22" />
                                <TextBlock Text="Age:" DockPanel.Dock="Right" Margin="0,0,3,0" />
                            </DockPanel>
                        </DockPanel>

                    </StackPanel>
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Country:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                            <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Region:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                            <Button   DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="City:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-23,0,10,0" Panel.ZIndex="1" Content="X" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-35,0,22,0" Panel.ZIndex="1" Content="..." />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Location info:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0"  />
                        </DockPanel>
                    </StackPanel>
                    <StackPanel view:MarginSetter.Margin="3" Orientation="Vertical" MinWidth="250" MinHeight="110" HorizontalAlignment="Left">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False">
                            <TextBlock Text="Hotel:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-30,0,17,0" Panel.ZIndex="1" Content="X" />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-39,0,29,0" Panel.ZIndex="1" Content="..." />
                            <Button  DockPanel.Dock="Right" MinWidth="10" Margin="-16,0,3,0" Panel.ZIndex="1" Content="O" />
                            <TextBox  DockPanel.Dock="Right" MinWidth="150" MinHeight="20" MaxWidth="150" Margin="0,0,10,0" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" VerticalAlignment="Center" >
                            <TextBlock Text="Room:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                            <TextBox  DockPanel.Dock="Right" Height="20" Width="157" Margin="0,0,5,0"  />
                        </DockPanel>
                        <DockPanel >
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Left" Margin="0">
                                <TextBlock Text="Date of Birth:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                                <DatePicker DockPanel.Dock="Right" Margin="8,0,0,0" Height="22" Width="90" FontSize="10" VerticalContentAlignment="Center" />
                            </DockPanel>
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Right" >
                                <TextBlock Text="Duration:" DockPanel.Dock="Right" Margin="0,0,7,0" />
                            </DockPanel>
                        </DockPanel>
                        <DockPanel >
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Left" Margin="0">
                                <TextBlock Text="Date of Birth:" DockPanel.Dock="Left" Margin="7,0,0,0" />
                                <DatePicker DockPanel.Dock="Right" Margin="8,0,0,0" Height="22" Width="90" FontSize="10" VerticalContentAlignment="Center" />
                            </DockPanel>
                            <DockPanel LastChildFill="False" VerticalAlignment="Center" Dock="Right" >
                                <TextBlock Text="5 Days" DockPanel.Dock="Right" Margin="0,0,15,0" />
                            </DockPanel>
                        </DockPanel>
                    </StackPanel>
                </StackPanel>
                <Separator HorizontalAlignment="Stretch" Height="1" Margin="0,9,0,0"/>
                <DockPanel>
                    <TextBlock DockPanel.Dock="Left" Text="Home Adrs:" Margin="16,10,0,0" />
                    <TextBox DockPanel.Dock="Left" Height="20" Width="498" Margin="1,10,153,0"  />
                </DockPanel>
                <Separator HorizontalAlignment="Stretch" Height="1" Margin="0,9,0,0"/>
                <StackPanel Orientation="Horizontal" Margin="0,7,0,0" >
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="5" Margin="16,0,0,0">
                        <DockPanel LastChildFill="False">
                            <TextBlock Text="Policy Num." DockPanel.Dock="Left" Margin="0" />
                            <TextBox  DockPanel.Dock="Right" Width="100" Height="21" MaxWidth="150" Margin="0,0,5,0" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" >
                            <TextBlock Text="Letter Code:" DockPanel.Dock="Left" Margin="0" />
                            <TextBox  DockPanel.Dock="Right" Width="100" MinHeight="20" Margin="10,0,5,0" />
                        </DockPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="5" Margin="16,0,0,0">
                        <DockPanel LastChildFill="False">
                            <TextBlock Text="Valid From:" DockPanel.Dock="Left" Margin="0" />
                            <DatePicker DockPanel.Dock="Right" Height="21" FontSize="10" Margin="8,0,0,0" Width="90" VerticalContentAlignment="Center" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" >
                            <TextBlock Text="Valid To:" DockPanel.Dock="Left" Margin="0" />
                            <DatePicker DockPanel.Dock="Right" Height="21" FontSize="10" Margin="8,0,0,0" Width="90" VerticalContentAlignment="Center" />
                        </DockPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="5" Margin="16,0,0,0">
                        <DockPanel LastChildFill="False">
                            <TextBlock Text="Insured Days:" DockPanel.Dock="Left" Margin="0" />
                            <TextBox  DockPanel.Dock="Right" Width="30" Height="21" MaxWidth="150" Margin="0,0,5,0" />
                        </DockPanel>
                        <DockPanel LastChildFill="False" Margin="3,8,3,5" >
                            <TextBlock Text="Insur. Programm:" DockPanel.Dock="Left" Margin="0" />
                            <TextBox  DockPanel.Dock="Right" Width="30" Height="21" Margin="10,0,5,0" />
                        </DockPanel>
                    </StackPanel>
                    <StackPanel Orientation="Vertical" view:MarginSetter.Margin="3" Margin="16,0,0,0">
                        <DockPanel LastChildFill="False">
