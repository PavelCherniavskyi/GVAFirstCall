                        </DockPanel>
                    </StackPanel>
                </StackPanel>
            </StackPanel>
        </Expander>
        <Expander Margin="5,0,5,5" BorderThickness="2" BorderBrush="Coral" IsExpanded="False" Padding="0,0,0,5">
            <Expander.Header>
                <TextBlock Text="Call info" FontWeight="Bold" FontSize="14" Height="22" Margin="0,5,0,0"/>
            </Expander.Header>
            <StackPanel Orientation="Horizontal" Margin="0,-11,0,-4">
                <StackPanel Orientation="Vertical" Margin="33,0,0,0" Width="450" view:MarginSetter.Margin="4">
                    <DockPanel>
                        <TextBlock Text="Reason for call:" Width="60" DockPanel.Dock="Left" TextTrimming="WordEllipsis" TextWrapping="Wrap" FontWeight="Bold"/>
                        <TextBox Width="350" Height="70" DockPanel.Dock="Right" Margin="-6,0,0,0" TextWrapping="Wrap" AcceptsReturn="True" ScrollViewer.VerticalScrollBarVisibility="Auto"/>
                    </DockPanel>
                    <StackPanel Orientation="Horizontal">
                        <DockPanel Width="158" Margin="76,0,0,0">
                            <CheckBox DockPanel.Dock="Left"/>
                            <TextBlock Text="This is chronical desease" DockPanel.Dock="Right" Margin="4,0,0,0"/>
                        </DockPanel>
                        <DockPanel Width="200">
                            <CheckBox DockPanel.Dock="Left"/>
                            <TextBlock Text="Alcohol or Drug usage within 24h" DockPanel.Dock="Left" Margin="4,0,0,0"/>
                        </DockPanel>
                    </StackPanel>
                </StackPanel>
                <StackPanel Orientation="Horizontal" Width="273"  Height="33" Margin="0,0,0,64">
                    <TextBlock Text="Status of call:" DockPanel.Dock="Left" Margin="26,0,0,0"  Height="22"/>
