            <StackPanel Orientation="Horizontal" Margin="0,-15,0,0" >
                <StackPanel Orientation="Vertical" view:MarginSetter.Margin="4" Margin="17,10,0,0" Width="298">
                    <StackPanel Orientation="Horizontal">
                        <DockPanel VerticalAlignment="Center" LastChildFill="False" Width="239">
                            <RadioButton GroupName="TreatDoc" Content="Outsourced doctor" DockPanel.Dock="Left"/>
                            <RadioButton GroupName="TreatDoc" Content="Medical Facility" DockPanel.Dock="Right"/>
